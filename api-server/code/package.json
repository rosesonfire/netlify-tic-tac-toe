{
  "name": "api-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "type-check": "tsc",
    "code-lint": "eslint . --ext .js,.ts",
    "filename-lint": "ls-lint",
    "lint": "yarn run filename-lint && yarn run code-lint",
    "print-eslint-config": "eslint --print-config ./src/server.ts",
